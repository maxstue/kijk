{
  "ConnectionStrings": {
    "DefaultConnection": "<from environment variable>"
  },
  "Persistence": {
    "SlowQueryLoggingEnabled": false,
    "SlowQueryLoggingThreshold": 1000
  },
  "AllowedHosts": "*",
  "Cors": ["https://kijk-app.justmax.xyz"],
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "System": "Warning",
        "Microsoft": "Information",
        "Microsoft.AspNetCore": "Warning",
        "Microsoft.AspNetCore.Hosting.Diagnostics": "Warning",
        "Microsoft.Hosting.Lifetime": "Information",
        "Microsoft.IdentityModel.LoggingExtensions.IdentityLoggerAdapter ": "Warning"
      }
    }
  },
  "Sentry": {
    "Dsn": "<from environment variable>",
    "SendDefaultPii": false,
    "MaxRequestBodySize": "Always",
    "MinimumBreadcrumbLevel": "Debug",
    "MinimumEventLevel": "Warning",
    "MaxBreadcrumbs": 50,
    "AttachStackTrace": true,
    "IncludeActivityData": true,
    "Debug": false,
    "DiagnosticLevel": "Error",
    "EnableTracing": true,
    "TracesSampleRate": 0.5
  }
}
