{
  "name": "kijk-web",
  "version": "0.1.0",
  "private": true,
  "keywords": [
    "nextjs",
    "pnpm",
    "tailwindcss",
    "shadcn/ui"
  ],
  "scripts": {
    "dev": "next dev -p 5003",
    "build": "next build",
    "start": "next start",
    "preview": "next build && next start",
    "lint": "eslint . --report-unused-disable-directives",
    "lint:knip": "knip",
    "lint:fix": "eslint . --report-unused-disable-directives --fix",
    "typecheck": "tsc --noEmit",
    "format": "prettier --check \"**/*.{ts,tsx,mdx}\"",
    "format:write": "prettier --write \"**/*.{ts,tsx,mdx}\"",
    "clean:all": "rimraf ./dist ./stats ./node_modules ./*.lock ./.eslintcache",
    "clean:dedupe": "pnpm dedupe --check",
    "clean:packages": "rimraf ./node_modules",
    "clean:build": "rimraf ./dist ./stats"
  },
  "dependencies": {
    "@radix-ui/react-slot": "^1.2.3",
    "@types/node": "^20.19.1",
    "clsx": "^2.1.1",
    "fumadocs-core": "12.5.0",
    "fumadocs-mdx": "8.2.33",
    "fumadocs-ui": "12.5.0",
    "lucide-react": "^0.400.0",
    "next": "^14.2.30",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tailwind-merge": "^2.6.0",
    "tailwind-variants": "^0.2.1",
    "zod": "^3.25.67"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.29.0",
    "@ianvs/prettier-plugin-sort-imports": "^4.4.2",
    "@types/mdx": "^2.0.13",
    "@types/react": "^18.3.23",
    "@types/react-dom": "^18.3.7",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "autoprefixer": "^10.4.21",
    "eslint": "8.50.0",
    "eslint-config-next": "13.5.4",
    "eslint-config-prettier": "^9.1.0",
    "globals": "^15.15.0",
    "knip": "^2.43.0",
    "postcss": "^8.5.6",
    "prettier": "^3.6.0",
    "prettier-plugin-tailwindcss": "^0.5.14",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.8.3"
  }
}
